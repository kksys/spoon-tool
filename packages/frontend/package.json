{
  "name": "spoon-tool-client",
  "private": true,
  "version": "0.0.12",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "gen-app-info": "cd $(git rev-parse --show-superproject-working-tree --show-toplevel | head -1) && ./build-scripts/generate-app-info",
    "prebuild": "run-p gen-app-info license",
    "dev": "pnpm prebuild && vite",
    "build": "pnpm prebuild && tsc -b && vite build",
    "circular": "depcruise src",
    "lint": "tsc --noEmit && eslint . --flag unstable_ts_config",
    "lint:fix": "eslint --fix . --flag unstable_ts_config",
    "format": "prettier ./**/*.{ts,tsx,cjs,html}",
    "format:fix": "prettier --write ./**/*.{ts,tsx,cjs,html}",
    "fix": "pnpm format:fix && pnpm lint:fix",
    "test": "echo 'Please add test commands here'",
    "preview": "vite preview",
    "i18next-extract": "i18next 'src/**/*.{tsx,ts}'",
    "live-server": "live-server --port=5173 ./dist/",
    "license": "./build-scripts/enumerate-license"
  },
  "dependencies": {
    "@fluentui/react-components": "^9.54.13",
    "@fluentui/react-icons": "^2.0.256",
    "@fluentui/react-nav-preview": "^0.7.4",
    "i18next": "^23.14.0",
    "inversify": "^6.0.2",
    "object-typed": "^1.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^15.0.1",
    "react-router": "^6.26.1",
    "react-router-dom": "^6.26.1",
    "react-use": "^17.5.1",
    "react-window": "^1.8.10",
    "react-window-infinite-loader": "^1.0.9",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@stylistic/eslint-plugin": "^2.8.0",
    "@stylistic/eslint-plugin-js": "^2.8.0",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^20.16.2",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@types/react-window": "^1.8.8",
    "@types/react-window-infinite-loader": "^1.0.9",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitejs/plugin-react-swc": "^3.7.0",
    "dependency-cruiser": "^16.4.1",
    "eslint": "^9.10.0",
    "eslint-plugin-react": "^7.36.1",
    "eslint-plugin-react-hooks": "5.1.0-rc-fb9a90fa48-20240614",
    "eslint-plugin-react-refresh": "^0.4.12",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "globals": "^15.9.0",
    "i18next-parser": "^9.0.2",
    "jiti": "^1.21.6",
    "license-checker": "^25.0.1",
    "live-server": "^1.2.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.3",
    "reflect-metadata": "^0.2.2",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.5.0",
    "vite": "^5.4.2",
    "vite-tsconfig-paths": "^5.0.1"
  }
}
