{
  "compilerOptions": {
    "composite": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.node.tsbuildinfo",
    "skipLibCheck": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "noEmit": true,
    "paths": {
      "eslint-plugin-react": ["./types/eslint-plugin-react.d.ts"],
      "eslint-plugin-react-hooks": ["./types/eslint-plugin-react-hooks.d.ts"],
      "eslint-plugin-react-refresh": ["./types/eslint-plugin-react-refresh.d.ts"],
    },
  },
  "include": [
    "src/i18n.resources.ts",
    "src/locales",
    "test/vitest.setup.ts",
    "types/eslint-plugin-react.d.ts",
    "types/eslint-plugin-react-hooks.d.ts",
    "types/eslint-plugin-react-refresh.d.ts",
    "eslint.config.ts",
    "i18next-parser.config.ts",
    "vite.config.ts",
    "vitest.config.ts",
  ]
}
